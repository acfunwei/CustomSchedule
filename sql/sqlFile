CREATE TABLE `fbp_sys_jobs`  (
  `ID` int(0) NOT NULL COMMENT '主键',
  `CREATE_DATE` datetime(0) NULL DEFAULT NULL COMMENT '创建日期',
  `CREATE_BY` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT '创建人',
  `UPDATE_DATE` datetime(0) NULL DEFAULT NULL COMMENT '更新日期',
  `UPDATE_BY` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT '更新人',
  `ENABLE_FLAG` int(0) NULL DEFAULT NULL COMMENT '是否启用',
  `JOB_CODE` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT '任务代码',
  `JOB_NAME` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT '任务名称',
  `JOB_TYPE` varchar(100) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT '任务类型',
  `CORN_EXPRESS` varchar(50) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT 'CORN表达式',
  `TIME_UNIT` varchar(20) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT '时间单元',
  `INTERVAL` int(0) NULL DEFAULT NULL COMMENT '时间间隔',
  `REMARKER` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_bin NULL DEFAULT NULL COMMENT '备注',
  `FINAL_EXEC_TIME` datetime(0) NULL DEFAULT NULL COMMENT '最后执行时间',
  `SYNC_DATE` datetime(0) NULL DEFAULT NULL COMMENT '同步时间',
  `PARAMETER` json NULL COMMENT '参数',
  PRIMARY KEY (`ID`) USING BTREE
);